<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axel or.com/xml/ns/object-views/object-views_5.2.xsd">

  <grid name="address-grid" title="address List" model="com.axelor.apps.base.db.Address" id="gst-address-grid" extension="true">    
     <field name="state"/>
   	
  </grid>

  <form name="address-form" title="address list"   model="com.axelor.apps.base.db.Address" id="gst-address-form" extension="true">
     <extend target="panel[@title='Address details']">
   <insert position="after">
      <panel>          
        <field name="state" colSpan="12"/>         
      </panel>
   </insert>
  </extend>
  </form>
    <form name="address-partner-address-form" title="partner address list"   model="com.axelor.apps.base.db.Address" id="gst-address-partner-address-form" extension="true">
     <extend target="panel[@title='Address details']">
   <insert position="after">
      <panel>          
        <field name="state" colSpan="12"/>         
      </panel>
   </insert>
  </extend>
  </form>
</object-views>
